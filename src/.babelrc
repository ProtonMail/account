{
    "presets": [
        [
            "preact-cli/babel",
            {
                "modules": "commonjs"
            }
        ],
        [
            "env",
            {
                "targets": {
                    "browsers": [
                        "ie 11"
                    ]
                }
            }
        ]
    ],
    "plugins": [
        [
            "babel-plugin-transform-react-jsx",
            {
                "pragma": "h"
            }
        ],
        "babel-plugin-transform-object-rest-spread",
        "babel-plugin-lodash",
        [
            "babel-plugin-transform-runtime",
            {
                "helpers": false,
                "polyfill": false,
                "regenerator": true
            }
        ],
        "transform-class-properties"
    ]
}
