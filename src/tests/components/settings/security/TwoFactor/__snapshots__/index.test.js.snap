exports[`TwoFactorSettings 2FA disabled 1`] = `
<div class="twoFactor">
  <h2>Two-Factor Authentication</h2>
  <div class="alert alert-info">Two-factor authentication is currently off.</div>
  <div id="totp" class="item">
    <div class="description">2FA via Application</div>
    <button class="action">Enable</button>
  </div>
  <div class="item">
    <div class="description">2FA via Security Key</div>
    <button class="action">Enable</button>
  </div>
  <ul class="list"></ul>
</div>
`;

exports[`TwoFactorSettings Everything enable 1`] = `
<div class="twoFactor">
  <h2>Two-Factor Authentication</h2>
  <div class="alert alert-info">Two-factor authentication is currently on.
    <button type="button" class="button">Turn off</button>
  </div>
  <div id="totp" class="item">
    <div class="description">2FA via Application</div>
    <button class="action">Disable</button>
    <div class="description">
      <button type="button" class="button">Regenerate recovery codes</button>
      <i data-toggle="tooltip" data-placement="top"
      title="Regenerate recovery codes will invalidate existing recovery codes">i</i>
    </div>
  </div>
  <div class="item lastItem">
    <div class="description">2FA via Security Key</div>
    <button class="action">Add another key</button>
  </div>
  <ul class="list">
    <li class="listElement">
      <div class="listElementHeader">First key</div>
      <button>Delete</button>
    </li>
    <li class="listElement">
      <div class="listElementHeader">Second key</div>
      <button>Delete</button>
    </li>
    <li class="listElement">
      <div class="listElementHeader listElementHeaderCompromised">Compromised key</div>
      <div class="badge badge-danger">Compromised</div>
      <button>Delete</button>
    </li>
    <li class="listElement">
      <div class="listElementHeader">Last key</div>
      <button>Delete</button>
    </li>
  </ul>
</div>
`;

